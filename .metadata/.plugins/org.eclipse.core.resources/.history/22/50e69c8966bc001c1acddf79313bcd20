import java.util.Scanner;

public class Ex04 {

	public static void main(String[] args) {

//		if문을 사용하여 5개 정수를 입력받아서 가장 큰 수를 출력하는 프로그램.

		Scanner sc = new Scanner(System.in);
		int num, max = 0;
		String tmp;
		
		for(int i = 0; i<5; i++) {
			tmp = sc.nextLine();
			num = Integer.parseInt(tmp);
			
			if(max<num) max  = num;
	
		} 
		System.out.println("최대값: " + max);
	
	}
}
