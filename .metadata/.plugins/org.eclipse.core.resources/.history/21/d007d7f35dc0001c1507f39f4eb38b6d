import java.util.Arrays;
import java.util.Scanner;

public class test {

	public static void main(String[] args) {
	
	
	int[] arr = { 9,6,7,3,5 };
		for(int i = 0; i<5; i++) {
			for(int j=0; j<arr.length; j++) {  
				if(arr[i] > arr[i]+1) { // 0 과 1자리 비교 시작.
				int tmp =0;
					arr[tmp] = arr[i];
					arr[i]= arr[j];
					arr[j]=arr[tmp];
				
				System.out.println(Arrays.toString(arr));	
				}
			}
		}
	} end
}
