import java.util.Scanner;

public class bus {

	public static void main(String[] args) {

		int age = (int)(Math.random()*99)+1; 
		
		int charge = 1500;
		double rate = 0;
		System.out.println("나이:" + age);
		
		if(0<=age && 3<=age || age >=65) {
			rate = charge*0;
			
			else if(4<=age && 13<=age) {
				rate = charge*0.5;
				
		}
		System.out.println("금액:" + (int)rate);
		
	}
}