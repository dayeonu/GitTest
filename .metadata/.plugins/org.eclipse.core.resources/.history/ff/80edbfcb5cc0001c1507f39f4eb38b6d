import java.util.Arrays;
import java.util.Scanner;

public class test {

	public static void main(String[] args) {
	
	
	int[] arr = { 9,6,7,3,5 };
		for(int i = 0; i<5; i++) {
			for(int j=0; j<i; j++) {
				if(arr[i] >arr[i]+3) { // 0 과 1자리 비교 시작.
					int tmp =0;
					arr[j]=arr[tmp];
					arr[tmp]= arr[i];
					arr[i]=arr[j];
					}	
				System.out.println(Arrays.toString(arr));	
		}
	} System.out.println(Arrays.toString(arr));
		//end of main
}}