import java.util.Arrays;
import java.util.Scanner;

public class test {

	public static void main(String[] args) {
	
	
	int[] arr = { 6,9,7,3,5 };
		for(int i = 0; i<5; i++) {
			for(int j=0; j<arr.length; j++) { // 0~5   
				for(; arr[i] > arr[i]+1; i++) { // 0 과 1자리 비교 시작.
				int tmp =0;
					arr[tmp] = arr[i];
					arr[i]= arr[j];
					arr[j]=arr[tmp];
				
				System.out.println(Arrays.toString(arr));	
				}
			}
		}
	} //end
}
